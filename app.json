{
  "expo": {
    "name": "bag-a-bake-frontend",
    "slug": "bag-a-bake-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logoBagABake.png",
    "scheme": "bagabake",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logoBagABake.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logoBagABake.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logoBagABake.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "baseURL": "http://10.233.2.18:3000/api"    
    }
  }
}