{
  "expo": {
    "name": "bag-a-bake-frontend",
    "slug": "bag-a-bake",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logoBagABake.png",
    "scheme": "bagabake",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logoBagABake.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logoBagABake.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.bagabakefrontend"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logoBagABake.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "baseURL": "http://10.125.50.78:3000/api",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "72d58b53-4510-4d53-84df-a915eb3f0bde"
      }
    },
    "owner": "patrick_roberto"
  }
}
