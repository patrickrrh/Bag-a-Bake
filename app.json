{
  "expo": {
    "name": "bag-a-bake-frontend",
    "slug": "bag-a-bake",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logoBagABake.png",
    "scheme": "bagabake",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logoBagABake.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.bagabakefrontend"
    },
    "android": {
      "permissions": ["INTERNET"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/foregroundBagabake.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.bagabakefrontend",
      "minSdkVersion": 24,
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logoBagABake.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "baseURL": "http://192.168.1.11:3000/api",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "72d58b53-4510-4d53-84df-a915eb3f0bde"
      }
    },
    "owner": "bag_a_bake"
  }
}
