{
  "expo": {
    "name": "Bag a Bake",
    "slug": "bag-a-bake",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logoBagABake.png",
    "scheme": "bagabake",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logoBagABake.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.bagabakefrontend"
    },
    "android": {
      "permissions": [
        "INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/foregroundBagabake.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.bagabakefrontend",
      "minSdkVersion": 24,
      "versionCode": 1,
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logoBagABake.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "baseURL": "http://bagabake.com/api",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cf209b3f-f03a-431a-9ab4-0b453a443209"
      }
    },
    "owner": "bag-a-bake"
  }
}
